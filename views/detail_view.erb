

  <! -- Begin Main About Section -->
  <section id="about" class="about-us content" style="margin-top: 34px">
    <div class="center">
      <!--start column_wrap-->
      <div class="columns">
        <div class="one_third">
          <img src="http://students.flatironschool.com/<%= @student.image %>" width="300px" alt="Sample Image">
        </div>
        <div class="two_third last">
          
          <h1><%=@student.name %></h1>

          <% if @student.tagline.length > 0 %>
            <h2><%=@student.tagline %></h2>
          <% end %>
            
          <% if @student.short.length > 0 %>  
            <p><%=@student.short %></p>
          <% end %>

          <% if @student.aspirations.length > 0 %> 
            <h3>Aspirations</h3>
            <p><%=@student.aspirations %></p>
          <% end %>

          <% if @student.interests.length > 0 %>
            <h3>Interests</h3>
            <p><%= @student.interests %></p>
          <% end %>

          <div class="social_icons">
            <ul>
              <li><a href="#"><i class="icon-linkedin"></a></i></li>
              <li><a href="http://walliswt.github.com"><i class="icon-globe"></a></i></li>
              <li><a href="https://twitter.com/WallisWT"><i class="icon-twitter"></a></i></li>
            </ul>
          </div>

        </div>
      </div>
    </div>
  </section>
  <!-- End Main About Section -->

  <div class="clear title rule" /></div>

